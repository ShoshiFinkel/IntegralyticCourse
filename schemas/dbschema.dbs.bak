		<procedure name="profits_last_day" id="Procedure-1b1a" isSystem="false" params_known="y" >
			<string><![CDATA[CREATE PROCEDURE dbo.profits_last_day
AS
SELECT (UnitPrice - (UnitPrice * Discount)) * Quantity AS total_price
FROM [Order Details] AS od
JOIN Orders o
ON o.OrderID = od.OrderID
WHERE o.OrderDate = (SELECT MAX(OrderDate) FROM Orders)]]></string>
			<result_param name="@RETURN_VALUE" jt="4" type="int" />
		</procedure>
		<procedure name="profits_yesterday" id="Procedure-21e0" isSystem="false" params_known="y" >
			<string><![CDATA[CREATE PROCEDURE dbo.profits_yesterday
AS
SELECT (od.UnitPrice - (od.UnitPrice * od.Discount)) * od.Quantity AS total_price
FROM [Order Details] od
JOIN Orders o
ON o.OrderID = od.OrderID
WHERE o.OrderDate = DATEADD(day,-1,GETDATE());
EXEC profits_yesterday;]]></string>
			<result_param name="@RETURN_VALUE" jt="4" type="int" />
		</procedure>
		<procedure name="year_orders" id="Procedure-11f7" isSystem="false" params_known="y" >
			<string><![CDATA[
CREATE PROCEDURE dbo.year_orders
@year VARCHAR(4)
AS
WITH five_products AS(
SELECT OrderID, COUNT(DISTINCT(ProductID)) product_amount
FROM [Order Details]
GROUP BY OrderID
)
SELECT o.OrderID, o.CustomerID, o.OrderDate 
FROM Orders o
JOIN five_products fp
ON o.OrderID = fp.OrderID
WHERE o.OrderDate like '%'+ @year + '%'
AND fp.product_amount >= 5;]]></string>
			<input_param name="@year" jt="12" type="varchar" inOut="1" />
			<result_param name="@RETURN_VALUE" jt="4" type="int" />
		</procedure>
